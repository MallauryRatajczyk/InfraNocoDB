server {
    listen 80;
    server_name rocketlacapsule.ddns.net;  # Remplace par ton domaine ou l'IP publique

    location / {
        proxy_pass http://10.10.1.2:32222;  # Adresse IP priv√©e de NocoDB dans ton VPC
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
}
